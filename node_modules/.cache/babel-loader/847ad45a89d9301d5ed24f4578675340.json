<div id=\"passwordForm\">\n        <!--        <button @click=\"forgot\">Forgot Password?</button> -->\n        <label for=\"password\">Password:</label>\n        <input type=\"text\" id=\"password\" name=\"password\" />\n        <button @click=\"logIn\">Log In</button>\n    </div>\n    <div id=\"registerForm\">\n        <label for=\"firstName\">First Name:</label>\n        <input type=\"text\" id=\"fName\" name=\"firstName\" />\n        <br />\n        <label for=\"lastName\">Last Name:</label>\n        <input type=\"text\" id=\"lName\" name=\"lastName\" />\n        <br />\n        <label for=\"phone\">Phone:</label>\n        <input type=\"number\" id=\"phone\" name=\"phone\" />\n        <br />\n        <label for=\"zipcode\">Zipcode:</label>\n        <input type=\"number\" id=\"zipcode\" name=\"zipcode\" />\n        <br />\n\n        <button @click=\"registeriBid\">Register for iBid</button>\n    </div>\n    <div id=\"registerEvent\">\n        <label for=\"cc\">Credit Card Number:</label>\n        <input type=\"number\" id=\"cc\" name=\"cc\" />\n        <br />\n        <label for=\"cc\">Billing Info:</label>\n        <input type=\"number\" id=\"cc\" name=\"cc\" />\n        <br />\n        <label for=\"cc\">Sercurity Code:</label>\n        <input type=\"number\" id=\"cc\" name=\"cc\" />\n        <br />\n\n        <button @click=\"registerEvent\">Register for this Event</button>\n    </div>\n</div>\n</template>\n\n<script>\nimport Vue from \"vue\";\n\nexport default {\n    components: {},\n    name: \"test\",\n    data() {\n        return {\n            user: {},\n            loggedIn: this.$store.state.user !== undefined,\n            requestedEvent: this.$store.state.event\n        };\n    },\n    computed: {\n        //hasher\n    },\n    methods: {\n        //check for iBid account\n        checkEmail() {\n            $(\"#next\").hide();\n            axios\n                .get(\"https://localhost:5001/api/users/email/\" + $(\"#email\").val())\n                .then(response => {\n                    // if email is associated with event, show password form\n                    // if email is not associated with event, show regsiter form\n                    // if email does not exist in database, make them register\n                    if (response.data.length > 0) {\n                        this.user = response.data[0];\n                        this.checkID(this.user.UserID);\n                        $(\"#passwordForm\").show();\n                    } else {\n                        $(\"#registerForm\").show();\n                    }\n                })\n                .catch(error => {\n                    console.log(error);\n                });\n        },\n        // check for Event Registration\n        checkID: function (id) {\n            axios\n                .get(\"https://localhost:5001/api/users/\" + id)\n                .then(response => {\n                    // array of Guest, EventIDs, isRegistered, guisExpress\n                    console.log(response.data[0].G);\n                    // if matching Event / isRegistered, redirect to homepage and login\n                })\n                .catch(error => {\n                    return error;\n                });\n        },\n        // log In to iBid\n        logIn() {\n            // use salt to hash password\n            var password = $(\"#password\").val() * this.user.salt;\n            if ((password = this.user.password)) {\n                this.loggedIn = true;\n            }\n        },\n        // register for iBid\n        registeriBid() {\n            var myData = {\n                \"email\": \"hello@gmail.com\",\n                \"password\": \"0x377F85DA7F1E4BA59FB1D0D59BB0D7F8E681204E6EE8AA2A49E0EC697D4D0DAE\",\n                \"salt\": \"XNWAYOBXN71DDGG482YYA9AK5H6HQK\",\n                \"fname\": $(\"#fName\").val(),\n                \"lname\": $(\"#lName\").val(),\n                \"zipcode\": $(\"#zipcode\").val(),\n                \"phone\": $(\"#phone\").val()\n            }\n            $.ajax({\n                type: \"POST\",\n                async: true,\n                dataType: \"json\",\n                contentType: \"application/json\",\n                url: \"https://localhost:5001/api/users\",\n                data: JSON.stringify(myData),\n                success: function (data) {\n                    console.log(\"Response Data â†“\");\n                    console.log(data);\n                },\n                error: function (err) {\n                    console.log(err);\n                }\n            });\n            // axios\n            //     .post(\n            //         \"https://localhost:5001/api/users\", {\n            //             email: \"hello\",\n            //             password: \"0x377F85DA7F1E4BA59FB1D0D59BB0D7F8E681204E6EE8AA2A49E0EC697D4D0DAE\",\n            //             salt: \"XNWAYOBXN71DDGG482YYA9AK5H6HQK\",\n            //             fname: $(\"#fName\").val(),\n            //             lname: $(\"#lName\").val(),\n            //             zipcode: $(\"#zipcode\").val(),\n            //             phone: $(\"#phone\").val()\n            //         }\n            //     )\n            //     .then(response => {\n            //         console.log(response);\n            //     })\n            //     .catch(error => {\n            //         console.log(error);\n            //     });\n        },\n        registerEvent() {}\n        // register for Event\n    },\n\n    mounted() {\n        if (this.loggedIn) {\n            $(\"#emailForm\").hide();\n        }\n        $(\"#passwordForm\").hide();\n        $(\"#registerForm\").hide();\n        $(\"#registerEvent\").hide();\n    }\n};\n</script>\n\n<style scoped>\n#login {\n    padding-top: 100px;\n    width: 100%;\n    align-content: center;\n    text-align: center;\n}\n</style>\n"],"sourceRoot":"src/views"}]}