,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA,EAAA,UAAA,EAAA,UAAA;AACA,QAAA,GAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA,EAAA,MAAA,EAAA,IAAA;AACA,QAAA,GAAA,CAAA,GAAA,CAAA,MAAA,CAAA,WAAA,EAAA,KAAA,EAAA,OAAA;AACA,OAhBA;AAiBA,KAxBA;AAyBA,IAAA,MAAA,EAAA,gBAAA,EAAA,EAAA;AAGA,UAAA,KAAA,GAAA,KAAA,UAAA,CAAA,SAAA,CAAA,UAAA,OAAA;AAAA,eAAA,OAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,CAAA,CAHA,CAIA;AACA;;AAEA,UAAA,KAAA,UAAA,CAAA,KAAA,EAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,UAAA,CAAA,KAAA,EAAA,MAAA,GAAA,IAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA;AAAA,UAAA,CAAA,EAAA;AAAA,SAAA;AACA,OAHA,MAGA,IAAA,KAAA,UAAA,CAAA,KAAA,EAAA,MAAA,KAAA,IAAA,EAAA;AACA,aAAA,UAAA,CAAA,KAAA,EAAA,MAAA,GAAA,KAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,gBAAA,EAAA;AAAA,UAAA,CAAA,EAAA;AAAA,SAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,KAxCA;AAyCA,IAAA,SAAA,EAAA,mBAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,GAAA,CAAA;AACA,UAAA,UAAA,GAAA,IAAA,GAAA,GAAA,GAAA,IAAA,EAAA,GAAA,EAAA,GAAA,IAAA;AACA,aAAA,UAAA;AACA;AA9CA;AAvDA,CAAA","sourcesContent":["<template>\n  <div id=\"list\" class=\"container\">\n    <div class=\"selection\" id=\"gap\">\n      <select v-model=\"selected\" id=\"dropdown\">\n        <option v-for=\"option in options\" v-bind:key=\"option.value\">{{ option.text }}</option>\n      </select>\n      <br />\n      <span v-if=\"selected\">Selected: {{ selected }}</span>\n    </div>\n\n    <div class=\"row\" v-for=\"data in filteredPosts\" :key=\"data.id\">\n      <div class=\"col-1\"></div>\n      <div class=\"card col-10\">\n        <div class=\"card-header\" ref=\"header\">\n          <div class=\"card-title\" style=\"position: relative\">\n            <router-link :to=\"{ name: 'post', params: {id: data.id}}\">\n              <h3 ref=\"items\">{{data.name}}</h3>\n            </router-link>\n            <div\n              class=\"heart\"\n              v-on:click=\"toggle(data.id)\"\n              v-bind:key=\"data.id\"\n              v-bind:style=\"heartHeight\"\n              v-bind:class=\"{amactive: activeKeys[activeKeys.findIndex((element) => element.id === data.id)].active}\"\n            ></div>\n          </div>\n        </div>\n        <div class=\"card-body row\" v-on:click=\"select($event)\" :id=\"data.id\">\n          <div class=\"col-5 d-flex align-items-center\">\n            <img class=\"card-image\" v-bind:src=\"getImage(data.image)\" />\n          </div>\n          <div class=\"col-7\">\n            <p v-if=\"data.sold\" class=\"card-text\">Sold Out!</p>\n            <div v-if=\"(!data.sold) && (timeUntil(data.end) > 0)\">\n              <p v-if=\"(timeUntil(data.end) > 0 )\" class=\"card-text\">Live!</p>\n              <p class=\"card-text\">Current Bid : {{data.price}}</p>\n              <p class=\"card-text\">Value : {{data.value}}</p>\n              <router-link :to=\"{ name: 'post', params: {id: data.id}}\">\n                <button class=\"btn btn-primary\">Bid Now!</button>\n              </router-link>\n            </div>\n          </div>\n          <div class=\"card-footer\" v-if=\"(timeUntil(data.end) > 0)\">\n            <countdown :time=\"timeUntil(data.end)\">\n              <div\n                slot-scope=\"props\"\n                class=\"date-text\"\n              >Bidding closes in {{ props.days }} days, {{ props.hours }} hours, {{ props.minutes }} minutes!</div>\n            </countdown>\n          </div>\n          <div class=\"card-footer\" v-if=\"(timeUntil(data.end) <= 0)\">Auction Over!</div>\n        </div>\n      </div>\n      <div class=\"col-1\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\n// Make a request for a user with a given ID\nimport datetime from \"datetime\";\nimport { mapState } from \"vuex\";\nimport Vue from \"vue\";\nimport VueCountdown from \"@chenfengyuan/vue-countdown\";\nimport cloudinary from \"cloudinary-core\";\n\nVue.component(VueCountdown.name, VueCountdown);\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      selected: \"All Items\",\n      options: [\n        { text: \"All Items\", value: \"1\" },\n        { text: \"Live Items\", value: \"2\" },\n        { text: \"Unsold Items\", value: \"3\" },\n        { text: \"Items with no bids\", value: \"4\" }\n      ],\n      times: [],\n      heartHeight: {}\n    };\n  },\n  mounted() {\n    this.$store.dispatch(\"loadPosts\");\n    this.getRowHeight();\n  },\n\n  computed: {\n    activeKeys() {\n      var array = [];\n      for (let i = 0; i < this.$store.state.posts.length; i++) {\n        array.push({\n          id: this.$store.state.posts[i].id,\n          active: this.$store.getters.findFavorite(\n            this.$store.state.posts[i].id\n          )\n        });\n      }\n      return array;\n    },\n    filteredPosts() {\n      if (this.selected === \"Live Items\") {\n        return this.$store.state.posts.filter(\n          post => this.timeUntil(post.end) > 0 === true\n        );\n      }\n      if (this.selected === \"Unsold Items\") {\n        return this.$store.state.posts.filter(\n          post => post.sold === false && this.timeUntil(post.end) > 0 === true\n        );\n      }\n      if (this.selected === \"Items with no bids\") {\n        return this.$store.state.posts.filter(\n          post =>\n            post.price === post.startingPrice &&\n            this.timeUntil(post.end) > 0 === true\n        );\n      } else return this.$store.state.posts;\n    },\n\n    ...mapState([\"posts\"])\n  },\n  methods: {\n    getImage: function(image) {\n      var cl = new cloudinary.Cloudinary({ cloud_name: \"kemp\", secure: true });\n      var tag = cl.url(image, { height: 200, width: 200, crop: \"fill\" });\n      return tag;\n    },\n    getRowHeight() {\n      this.$nextTick(() => {\n        //sizing\n        let target = this.$refs.items[0].clientHeight;\n        let factor = target / 100;\n        let string = \"scale(\" + 2 * factor + \")\";\n        Vue.set(this.heartHeight, \"transform\", string);\n\n        // positioning\n        const left =\n          -this.$refs.header[0].getBoundingClientRect().left + target + \"px\";\n\n        let header = this.$refs.header[0].clientHeight;\n        const top = -this.$refs.header[0].getBoundingClientRect().top + \"px\";\n        Vue.set(this.heartHeight, \"position\", \"absolute\");\n        Vue.set(this.heartHeight, \"left\", \"0%\");\n        Vue.set(this.heartHeight, \"top\", \"-100%\");\n      });\n    },\n    toggle: function(id) {\n   \n\n      var index = this.activeKeys.findIndex(element => element.id === id);\n      // this.activeKeys[this.activeKeys.findIndex(element => element.id === id)].active =\n      // !this.activeKeys[this.activeKeys.findIndex(element => element.id === id)].active;\n\n      if (this.activeKeys[index].active === false) {\n        this.activeKeys[index].active = true;\n        this.$store.dispatch(\"setFavorite\", { n: id });\n      }  else if (this.activeKeys[index].active === true) {\n        this.activeKeys[index].active = false;\n        this.$store.dispatch(\"removeFavorite\", { n: id });\n      }\n      console.log(this.$store.state.favorites)\n    },\n    timeUntil: function(end) {\n      const now = new Date();\n      const then = new Date(end);\n      const difference = then - now + 5 * 60 * 60 * 1000;\n      return difference;\n    }\n  }\n};\n</script>\n\n<style scoped  lang=\"scss\">\n@media (max-height: 600px) {\n  #gap {\n    background-color: none;\n  }\n  .container {\n    position: absolute;\n    top: 100px;\n  }\n  .card-text {\n    font-size: 14px;\n  }\n  .date-text {\n    font-size: 14px;\n    margin: 0 5% 0 5%;\n  }\n}\n\n@media (min-height: 600px) {\n  #gap {\n    background-color: none;\n  }\n  .card-text {\n    font-size: 14px;\n  }\n  .date-text {\n    font-size: 14px;\n  }\n  .container {\n    position: absolute;\n    top: 100px;\n  }\n  .card-title {\n    font-size: 18px;\n  }\n}\n@media (min-height: 700px) {\n  #gap {\n    background-color: none;\n  }\n  .container {\n    position: absolute;\n    top: 100px;\n  }\n  .card-text {\n    font-size: 14px;\n  }\n  .card-title {\n    font-size: 24px;\n  }\n  .date-text {\n    font-size: 14px;\n  }\n  .card-title {\n    font-size: 20px;\n  }\n}\n@media (min-height: 900px) {\n  #gap {\n    background-color: none;\n  }\n  .btn {\n    font-size: 25px;\n  }\n  .card-title {\n    font-size: 64px;\n  }\n  .card-text {\n    font-size: 24px;\n  }\n  .date-text {\n    font-size: 20px;\n  }\n  .card {\n    width: calc(var(--vh, 1vh) * 40);\n  }\n  .container {\n    position: absolute;\n    top: 120px;\n  }\n\n  .selection {\n    font-size: 24px;\n  }\n}\n.card-header {\n  padding: 2.5%;\n  width: 100%;\n  margin-bottom: 5%;\n}\n.card-body {\n  width: 100%;\n  margin: 0;\n  padding: 0;\n}\n.card-footer {\n  padding: 0px;\n  padding-bottom: 2.5%;\n  margin-top: 5%;\n  align-content: center;\n  width: 100%;\n}\n.card-title {\n  margin-top: 5%;\n  margin: 0;\n}\n.card {\n  margin-bottom: 10%;\n  padding: 0;\n  border: black 0.5px solid;\n  background-color: #bfdbf7;\n}\nh3,\np {\n  color: black;\n  margin-bottom: 0.25em;\n}\nimg {\n  width: 100%;\n}\n\nbutton {\n  width: 75%;\n}\n\n.btn-primary {\n  background-color: #1f7a8c;\n  border-color: #343a40;\n  color: white;\n  margin: 5px;\n}\n\n.selection {\n  margin-bottom: 10%;\n}\n\n#dropdown {\n  margin-bottom: 5%;\n}\n\n// TWITTER HEART\n.heart {\n  position: absolute;\n  z-index: 2;\n  // top: -14%;\n  // right: 0%;\n  width: 100px;\n  height: 100px;\n  background: url(\"https://cssanimation.rocks/images/posts/steps/heart.png\")\n    no-repeat;\n  background-position: 0 0;\n  cursor: pointer;\n  transition: background-position 1s steps(28);\n  transition-duration: 0s;\n\n  &.amactive {\n    transition-duration: 1s;\n    background-position: -2800px 0;\n  }\n}\n\n// BASIC\nbody {\n  background: linear-gradient(135deg, #121721 0%, #000000 100%) fixed;\n  color: #fff;\n  font: 300 16px/1.5 \"Open Sans\", sans-serif;\n}\n</style>"],"sourceRoot":"src/views"}]}