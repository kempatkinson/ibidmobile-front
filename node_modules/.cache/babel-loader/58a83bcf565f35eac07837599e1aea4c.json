ITiB,IAAAA,QAJS,oBAIAlB,KAJA,EAIOe,CAJP,EAIU;AACjBf,MAAAA,KAAK,CAACE,SAAN,CAAgBiB,IAAhB,CAAqBJ,CAArB;AACAH,MAAAA,OAAO,CAACC,GAAR,CAAY,eAAZ,EAA6Bb,KAAK,CAACE,SAAnC;AACD;AAPQ,GAvBiB;AAgC5BkB,EAAAA,OAAO,EAAE;AACPC,IAAAA,IAAI,EAAE,cAACrB,KAAD;AAAA,aAAW,UAACsB,EAAD,EAAQ;AACvB,eAAOtB,KAAK,CAACC,KAAN,CAAYsB,IAAZ,CAAiB,UAAAC,CAAC;AAAA,iBAAIA,CAAC,CAACF,EAAF,KAASA,EAAb;AAAA,SAAlB,CAAP;AACD,OAFK;AAAA;AADC;AAhCmB,CAAf,CAAf","sourcesContent":["import Vue from \"vue\";\nimport Vuex from \"vuex\";\nimport axios from 'axios'\nimport createPersistedState from \"vuex-persistedstate\";\n\n\nVue.use(Vuex);\n\nexport default new Vuex.Store({\n  state: {\n    posts: [],\n    favorites: [],\n  },\n  plugins: [createPersistedState()],\n  actions: {\n    loadPosts({ commit }) {\n      axios.get(\"https://afternoon-taiga-12401.herokuapp.com/api/biditems\")\n      // axios.get(\"http://localhost:5000/api/biditems\")\n        .then(response => response.data)\n        .then(posts => {\n          commit(\"GET_POSTS\", posts)\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    },\n    setFavorite({ commit }, n) {\n      commit(\"favorite\", n);\n      console.log('action hit');\n    },\n  },\n  mutations: {\n    GET_POSTS(state, posts) {\n      state.posts = posts;\n    },\n    favorite(state, n) {\n      state.favorites.push(n);\n      console.log(\"favorites:   \", state.favorites)\n    }\n  },\n  getters: {\n    post: (state) => (id) => {\n      return state.posts.find(p => p.id === id)\n    }\n  }\n});\n"]}]}