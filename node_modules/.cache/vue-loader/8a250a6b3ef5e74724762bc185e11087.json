eturn {\n      events: []\n    };\n  },\n  mounted() {\n    this.getEvents();\n  },\n  methods: {\n    async getEvents() {\n      const url = \"https://localhost:5001/api/events/\";\n      await axios\n        .get(url)\n        .then(response => (this.events = response.data))\n        .catch(error => {\n          console.log(error);\n        });\n\n      for (var i = 0; i < this.events.length; i++) {\n        var posts = [];\n        let url = \"https://localhost:5001/api/events/\" + this.events[i].TinyID;\n        console.log(url);\n        axios\n          .get(url)\n          // \"https://afternoon-taiga-12401.herokuapp.com/api/biditems\"\n          .then(response => (\n            posts = response.data),\n            console.log(response.data))\n\n          .catch(error => {\n            console.log(\"Hello\");\n          });\n\n        // if ((posts.length = 0)) {\n        //   this.events.splice(i, 1);\n        // }\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n#welcome {\n  position: absolute;\n  top: 100px;\n  margin-left: 10%;\n  margin-right: 10%;\n}\n</style>"]}]}