;AAAA,SAAlB,CAAP;AACD,OAFK;AAAA;AADC;AAvBmB,CAAf,CAAf","sourcesContent":["import Vue from \"vue\";\nimport Vuex from \"vuex\";\nimport axios from 'axios'\n\nVue.use(Vuex);\n\nexport default new Vuex.Store({\n  state: {\n    posts: [],\n  },\n  actions: {\n    loadPosts ({ commit }) {\n      axios.get('/api/BidItems')\n        .then(response => response.data)\n        .then(posts => {\n          commit(\"GET_POSTS\", posts)\n        })\n        .catch(error => {\n          console.log(error);\n        });\n\n\n    }\n  },\n  mutations: {\n    GET_POSTS (state, posts) {\n      state.posts = posts;\n    }\n  },\n  getters: {\n    post: (state) => (id) => {\n      return state.posts.find(p => p.id === id)\n    }\n  }\n});\n"]}]}